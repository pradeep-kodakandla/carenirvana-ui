{
  "name": "carenirvana.ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "ng serve --port 4200 --proxy-config proxy.conf.json",
    "start:windows": "ng serve --port 44404 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key --proxy-config proxy.conf.json",
    "start:default": "ng serve --port 44404 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:ssr": "ng run CareNirvana.UI:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
